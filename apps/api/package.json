{
  "name": "@neonhub/backend-v3.2",
  "version": "3.2.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "packageManager": "npm@10.8.2",
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  },
  "scripts": {
    "prisma:generate": "node ../../scripts/run-cli.mjs prisma generate || node ../../scripts/offline-prisma-generate.mjs",
    "prisma:deploy": "node ../../scripts/run-cli.mjs prisma migrate deploy",
    "prisma:seed": "node ../../scripts/run-cli.mjs prisma db seed",
    "prisma:migrate:deploy": "node ../../scripts/run-cli.mjs prisma migrate deploy",
    "prisma:migrate": "node ../../scripts/run-cli.mjs prisma migrate dev",
    "prisma:studio": "node ../../scripts/run-cli.mjs prisma studio",
    "prisma:validate": "node ../../scripts/run-cli.mjs prisma validate",
    "prisma:status": "node ../../scripts/run-cli.mjs prisma migrate status --schema apps/api/prisma/schema.prisma",
    "db:drift-check": "npm run prisma:validate && npm run prisma:status",
    "build": "npm run prisma:generate && node ../../scripts/run-cli.mjs tsc -p .",
    "start": "node dist/src/server.js",
    "dev": "node ../../scripts/run-cli.mjs tsx watch src/server.ts",
    "test": "NODE_ENV=test node ../../scripts/run-cli.mjs jest",
    "test:coverage": "NODE_ENV=test node ../../scripts/run-cli.mjs jest --coverage --coverageDirectory=coverage",
    "test:watch": "NODE_ENV=test node ../../scripts/run-cli.mjs jest --watch",
    "lint": "node ../../scripts/run-cli.mjs eslint src --ext .ts",
    "typecheck": "node ../../scripts/run-cli.mjs tsc --noEmit",
    "format": "node ../../scripts/run-cli.mjs prettier --write \"src/**/*.ts\"",
    "format:check": "node ../../scripts/run-cli.mjs prettier --check \"src/**/*.ts\"",
    "verify": "npm run lint && npm run typecheck && npm run test",
    "seed": "node ../../scripts/run-cli.mjs tsx prisma/seed.ts",
    "prepare": "npm run prisma:generate"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.0",
    "@neonhub/predictive-engine": "file:../../modules/predictive-engine/neonhub-predictive-engine-3.2.0.tgz",
    "@prisma/client": "^5.22.0",
    "@sentry/node": "^10.17.0",
    "@sentry/profiling-node": "^10.17.0",
    "@tensorflow/tfjs": "^4.16.0",
    "@types/redis": "^4.0.10",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ethers": "^6.7.0",
    "express": "^4.19.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^7.1.0",
    "openai": "^4.68.4",
    "pdfkit": "^0.15.0",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "puppeteer": "^23.5.0",
    "redis": "^5.8.3",
    "resend": "^6.1.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "twilio": "^5.3.3",
    "uuid": "^13.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.12.12",
    "@types/pdfkit": "^0.17.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^5.22.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.16.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
